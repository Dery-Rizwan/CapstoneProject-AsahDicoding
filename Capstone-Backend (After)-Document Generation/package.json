{
  "name": "ba-digital-backend",
  "version": "1.0.0",
  "description": "Backend API for BAPB & BAPP System - Unified",
  "main": "src/app.js",
"scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "init": "node src/utils/initUploads.js",
    "setup": "npm run init && npm run db:migrate",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:seed": "sequelize-cli db:seed:all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["bapb", "bapp", "berita-acara", "api"],
  "author": "Team A25-CS144",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "sequelize": "^6.35.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "pdfkit": "^0.13.0",      
    "uuid": "^9.0.1",
    "docx": "^8.5.0",         
    "moment": "^2.30.1"       
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2"
  }
}